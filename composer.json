{
  "name": "skuads/laravel-google-my-business",
  "description": "A package for Laravel which implements the Google My Business API",
  "keywords": [
    "google",
    "google my business",
    "laravel",
    "laravel5"
  ],
  "license": "Apache-2.0",
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0 | ^11.0 | ^12.0",
    "google/apiclient": "^2.15",
    "google/apiclient-services": "^0.376.0"
  },
  "autoload": {
    "psr-4": {
      "Skuads\\LaravelGoogleMyBusiness\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Skuads\\LaravelGoogleMyBusiness\\GoogleMyBusinessServiceProvider"
      ],
      "aliases": {
        "GoogleMyBusiness": "Skuads\\LaravelGoogleMyBusiness\\Facades\\GoogleMyBusinessFacade"
      }
    }
  }
}
